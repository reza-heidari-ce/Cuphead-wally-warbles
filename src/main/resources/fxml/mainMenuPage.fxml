<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="view.MainMenuPage"
            prefHeight="400.0" prefWidth="600.0" fx:id="pane" stylesheets="@../css/style.css">

    <center>
        <VBox alignment="CENTER" spacing="10">

            <Button text="New Game" onMouseClicked="#newGame"/>
            <Button text="Profile" onMouseClicked="#enterProfile"/>
            <Button text="ScoreBoard" onMouseClicked="#enterScoreBoard" />
            <Button text="Exit" onMouseClicked="#exit"/>
        </VBox>
    </center>

</BorderPane>
